  },
  "betterScripts": {
    "build": {
      "command": "webpack --config ./webpack/webpack.config.prod.js --colors --display-error-details --verbose",
      "env": {
        "NODE_ENV": "production",
        "BABEL_ENV": "production"
      }
    },
    "start": {
      "command": "http-server dist/",
      "env": {
        "NODE_ENV": "production",
        "BABEL_ENV": "production"
      }
    },
    "watch": {
      "command": "webpack-dev-server --config ./webpack/webpack.config.dev.js",
      "env": {
        "NODE_ENV": "development",
        "BABEL_ENV": "development"
      }
    }
  },
